{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/utils/src/browser.ts",
    "packages/utils/src/node.ts",
    "packages/utils/src/index.ts",
    // "packages/utils/types/index.d.ts",
  ],
  "cleanOutputDir": true,
  "emit": "docs",
  "entryPointStrategy": "expand",
  "gitRevision": "develop",
  "excludeExternals": true,
  "excludeNotDocumented": false,
  "excludeReferences": false,
  "externalPattern": ["**/node_modules/**"],
  "githubPages": true,
  "includeVersion": true,
  "out": "./docs",
  "skipErrorChecking": true,
  "treatWarningsAsErrors": false,
  "tsconfig": "tsconfig.typedoc.json",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "plugin": ["typedoc-plugin-merge-modules", "typedoc-plugin-coverage"],
  "coverageLabel": "coverage",
  "mergeModulesRenameDefaults": false, // NEW option of TypeDoc added by this plugin
  "mergeModulesMergeMode": "module" // NEW option of TypeDoc added by this plugin
}
