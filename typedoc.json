{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["packages/*/docs"],
  "out": "./docs",
  "excludeExternals": true,
  "includeVersion": true,
  "tsconfig": "tsconfig.json",
  "emit": "docs",
  "entryPointStrategy": "merge",
  "skipErrorChecking": true,
  "githubPages": true,
  "treatWarningsAsErrors": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  }
}
