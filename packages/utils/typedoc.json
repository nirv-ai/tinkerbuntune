{
  "extends": ["../../typedoc.json"],
  "tsconfig": "tsconfig.typedoc.json",
  "entryPoints": ["src/node.ts", "src/browser.ts", "src/bun.ts"],
  "json": "./docs/exports.json",
  "entryPointStrategy": "resolve",
  "skipErrorChecking": true,
  "excludeNotDocumented": false,
  "treatWarningsAsErrors": false,
  "cleanOutputDir": true,
  "githubPages": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  }
}
