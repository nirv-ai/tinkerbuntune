{
  "name": "bun",
  "version": "0.3.8",
  "private": true,
  "type": "module",
  "types": "./types/src/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "default": "./js/index.js"
    }
  },
  "scripts": {
    "build:js": "bun build --entrypoints ./src/index.ts --outdir ./js --target browser --minify --external bun:jsc",
    "examples": "bun --watch src/examples/airRoutes.ts",
    "test:types": "rm -rf types/*buildinfo && bunx tsc"
  },
  "devDependencies": {
    "@types/gremlin": "3.6",
    "bun-types": "latest",
    "csv": "6.3.5",
    "gremlin": "3.6.2",
    "type-fest": "4.8"
  },
  "peerDependencies": {
    "@types/gremlin": "3.6",
    "csv": "6.3.5",
    "gremlin": "3.6.2",
    "type-fest": "4.8",
    "typescript": "5.2"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2"
  },
  "bundledDependencies": [
    "@msgpack/msgpack"
  ]
}
